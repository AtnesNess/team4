<!-- htmllint attr-name-style="false" -->
{{>photo photo=photo blockClass=''}}
{{>geo-button blockClass=''}}
{{>general-button text='удалить' style='primary' blockClass='delete-btn'}}
<input type="checkbox" class="edited" name="photo-block-{{index}}-edited"/>
<input type="checkbox" class="deleted" name="photo-block-{{index}}-deleted"/>
<input name="photo-block-{{index}}-photo" class="photo-link" value="{{photo.url}}"/>
<input name="photo-block-{{index}}-location"
    value="{{photo.geoPosition.lat}}, {{photo.geoPosition.lng}}" class="photo-location"/>
